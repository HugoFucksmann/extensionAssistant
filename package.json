{
  "name": "extensionassistant",
  "displayName": "Extension Assistant",
  "description": "Asistente de IA para VS Code",
  "version": "1.0.0",
  "publisher": "user",
  "engines": {
    "vscode": "^1.96.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ai-chat-sidebar",
          "title": "AI Chat",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "ai-chat-sidebar": [
        {
          "type": "webview",
          "id": "aiChat.chatView",
          "name": "AI Chat",
          "icon": "$(comment-discussion)"
        }
      ]
    },
    "configuration": {
      "title": "Extension Assistant",
      "properties": {
        "extensionAssistant.modelType": {
          "type": "string",
          "enum": [
            "ollama",
            "gemini"
          ],
          "default": "gemini"
        },
        "extensionAssistant.persistChat": {
          "type": "boolean",
          "default": true
        },
        "extensionAssistant.useOrchestration": {
          "type": "boolean",
          "default": false
        }
      }
    },
    "commands": [
      {
        "command": "extensionAssistant.openChat",
        "title": "Abrir Chat de IA"
      },
      {
        "command": "extensionAssistant.newChat",
        "title": "Nuevo Chat",
        "icon": "$(add)"
      },
      {
        "command": "extensionAssistant.settings",
        "title": "Configuraci√≥n del Asistente",
        "icon": "$(gear)"
      },
      {
        "command": "extensionAssistant.chat.history",
        "title": "Mostrar Historial de Chats",
        "icon": "$(history)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "extensionAssistant.newChat",
          "when": "view == aiChat.chatView",
          "group": "navigation@1"
        },
        {
          "command": "extensionAssistant.chat.history",
          "when": "view == aiChat.chatView",
          "group": "navigation@2"
        },
        {
          "command": "extensionAssistant.settings",
          "when": "view == aiChat.chatView",
          "group": "navigation@3"
        }
      ]
    }
  },
  "scripts": {
    "build": "node build.mjs",
    "build:prod": "node build.mjs --production",
    "watch": "node build.mjs --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./src --ext .ts,.tsx,.js,.jsx",
    "test": "jest",
    "check": "npm run lint && npm run typecheck && npm run test",
    "compile": "node build.mjs"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.48",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.27.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google/generative-ai": "^0.21.0",
    "@langchain/core": "^0.3.57",
    "@langchain/google-gauth": "^0.2.9",
    "@langchain/google-genai": "^0.2.9",
    "@langchain/langgraph": "^0.2.72",
    "@langchain/ollama": "^0.2.0",
    "eventemitter3": "^5.0.1",
    "langchain": "^0.3.27",
    "markdown-to-jsx": "^7.7.2",
    "prismjs": "^1.29.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "sqlite3": "^5.1.7",
    "tree-sitter-javascript": "^0.23.1",
    "tree-sitter-typescript": "^0.23.2",
    "web-tree-sitter": "^0.25.1",
    "zod": "^3.25.34"
  }
}
